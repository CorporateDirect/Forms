{
  "name": "form-functionality-library",
  "version": "1.1.0",
  "description": "A modular, flexible form functionality library for Webflow forms supporting single-step, multi-step, and branching forms",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc && terser dist/index.js --compress --mangle --output dist/index.min.js",
    "minify": "terser dist/index.js --compress --mangle --output dist/index.min.js",
    "dev": "tsc --watch",
    "serve": "python3 -m http.server 8000",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "forms",
    "webflow",
    "multi-step",
    "branching",
    "validation",
    "typescript"
  ],
  "author": "Form Functionality Library",
  "license": "MIT",
  "devDependencies": {
    "terser": "^5.43.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/form-functionality-library.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/form-functionality-library/issues"
  },
  "homepage": "https://github.com/your-username/form-functionality-library#readme"
}
